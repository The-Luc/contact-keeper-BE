      return {\n        ...state,\n        filtered: state.contacts.filter(contact => {\n          const regex = new RegExp(`${action.payload}`, 'gi');\n          return contact.name.match(regex) || contact.email.match(regex);\n        }),\n      };\n    case CLEAR_FILTER:\n      return {\n        ...state,\n        filtered: null,\n      };\n    default:\n      return state;\n  }\n};\n\nexport default contactReducer;\n",[],"/home/lucthi/frontend/learn/react/contact-keeper/client/src/components/contacts/Contacts.js",[],[],"/home/lucthi/frontend/learn/react/contact-keeper/client/src/components/contacts/ContactItem.js",[],[],"/home/lucthi/frontend/learn/react/contact-keeper/client/src/components/contacts/ContactForm.js",[],[],"/home/lucthi/frontend/learn/react/contact-keeper/client/src/components/contacts/ContactFilter.js",[],[],{"ruleId":"71","severity":1,"message":"72","line":7,"column":3,"nodeType":"73","messageId":"74","endLine":7,"endColumn":17},{"ruleId":"71","severity":1,"message":"75","line":8,"column":3,"nodeType":"73","messageId":"74","endLine":8,"endColumn":15},"no-unused-vars","'FILTER_CONTACT' is defined but never used.","Identifier","unusedVar","'CLEAR_FILTER' is defined but never used."]